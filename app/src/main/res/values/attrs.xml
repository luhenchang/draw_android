<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CurveImageColorView">
        <attr name="defImg" format="reference" />
    </declare-styleable>

    <declare-styleable name="CurvedBottomView">
        <attr name="curvedStartY" format="dimension" />
        <attr name="curvedControllerY" format="dimension" />
        <attr name="customBackground" format="reference|color" />
    </declare-styleable>

    <declare-styleable name="BottomBarNavigationView">
        <attr name="icons" format="reference" />
        <attr name="iconSize" format="dimension" />
        <attr name="cubicMargin" format="dimension" />
        <attr name="circleRadius" format="dimension" />
        <attr name="bgColor" format="color" />
    </declare-styleable>

    <declare-styleable name="ProgressBarView">
        <attr name="progressScale" format="float" />
        <attr name="isDragProgress" format="boolean" />

    </declare-styleable>

    <declare-styleable name="ProgressBallView">
        <attr name="progressBallScale" format="float" />
    </declare-styleable>
</resources>